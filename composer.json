{
    "name": "bmartinGithub/slim-mvc-skeleton",
    "description": "A Slim Framework skeleton application for rapid development forked from slimphp/skim-skeleton.",
    "keywords": ["microframework", "rest", "router", "psr7", "mvc"],
    "homepage": "https://github.com/bmartinGithub/Slim-MVC-Skeleton.git",
    "license": "MIT",
    "authors": [
        {
            "name":"Bruce Martin",
            "email":"bruce@martinsolution.com",
            "homepage":"http://you-dont-want-to-see-that-its-way-out-dated.com"
        },
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "illuminate/database": "~5.1",
        "slim/twig-view": "^2.4.0",
        "slim/csrf": "^0.8.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\":"app/",
            "App\\Views\\":"app/views",
            "App\\Controllers\\":"app/controllers",
            "App\\Lib\\ControllerTraits\\":"app/lib/controllerTraits",
            "App\\Models\\":"app/models",
            "App\\Lib\\":"app/lib"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
